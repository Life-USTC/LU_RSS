    <script>
        var ctx = "";

        function clickHref(target, linkType, link, source, id) {
            //文章
            if (linkType == '1') {
                window.open(ctx + "/article/" + id, target);
            }
            //文件
            if (linkType == '2') {
                window.open(source);
            }
            //外链
            if (linkType == '3') {
                window.open(link, target);
            }
        }
    </script>

    <script>
        (function ($) {
            var SwiperPlugin = {};
            // 初始swiper
            SwiperPlugin.initSwiper = function () {
                new Swiper('#banner', {
                    loop: true,
                    pagination: '.swiper-pagination',
                    paginationClickable: true,
                    grabCursor: true,
                    autoplay: 4000,
                    nextButton: '.swiper-button-next',
                    prevButton: '.swiper-button-prev',
                });
                return this
            };

            SwiperPlugin.initSwiper2 = function () {
                new Swiper('.slider-ul', {
                    slidesPerView: 8,
                    // spaceBetween: 30,
                    nextButton: '#next-arrow',
                    prevButton: '#prev-arrow'
                });
                return this
            };
            SwiperPlugin.init = function () {
                this
                    .initSwiper()
                    .initSwiper2();
                console.log('init');
            };
            SwiperPlugin.init();
        })(jQuery);
    </script>
    <script>

        /* $('.btn-span-h2 > span').each(function (e) {
             $(this).hover(function () {
                 $('.btn-span-h2 > span').removeClass('active')
                 $(this).addClass('active')
                 $(this).parent().parent().next().find('ul').removeClass('show-ul-active')
                 $(this).parent().parent().next().find('ul').eq(e).addClass('show-ul-active')
             })
         })*/
        $('.btn-span-h2 > span').hover(function () {
            let index = $(this).index()
            $(this).addClass('active').siblings().removeClass('active')
            $(this).parent().parent().next().find('ul').eq(index).addClass('show-ul-active').siblings().removeClass('show-ul-active')
        })

        /*  <!--                    2020-03-21-->*/
    </script>
