    $(".px_mb_tablist").on("click", function () {
        $(this).addClass("curr").siblings().removeClass("curr");
        $(".l_active_item").eq($(this).index()).show().siblings(".l_active_item").hide();
        mychw.resize();
        mychw3.resize();
    })
    window.addEventListener("resize", function () {
        mychw3.resize();
        mychw.resize();
    });
</script>
</body>
</html>