$('#slider1').bxSlider({
  mode: 'fade',
  speed: 1000,
  auto: true,
  pager: true,
  controls: false //隐藏左右按钮
});

$(function () {
var sliderSmhtml =$("#slider2").html();
  // 小轮播
  var sliderSm = $('.slider2').bxSlider({
    slideWidth: 260,
    auto: true,
    infiniteLoop: true,
    autoControls: true,
    pager: false,
    moveSlides: 1,
    minSlides: 4,
    maxSlides: 4,
    pause: 3000,
    slideMargin: 20
  });
  // 是否轮播
 
 setTimeout(function () {
    if (sliderSm.getSlideCount() <= 4) {
      sliderSm.destroySlider();
    }
  }, 60);
// slider-tab切换
  $(document).on("click", ".l-tab-box li", function () { 	
	console.log(sliderSmhtml)
    	$(this).addClass("active").siblings().removeClass("active")
        $(this).parents(".w_head_main").next().find(".l-course-box").html("")
    	if ($(this).index() == 0) {
			var html ='<ul class="smallSlider clearf slider2">'+ sliderSmhtml+'</ul>'
    		$(this).parents(".w_head_main").next().find(".l-course-box").append(html);
    	}else if ($(this).index() == 1) {
            var html ='<ul class="smallSlider clearf slider2">'+ $("#picture_2").html()+'</ul>'
    		$(this).parents(".w_head_main").next().find(".l-course-box").append(html);
    	}else if ($(this).index() == 2) {
			var html ='<ul class="smallSlider clearf slider2">'+ $("#picture_3").html()+'</ul>'
    		$(this).parents(".w_head_main").next().find(".l-course-box").append(html);
    	}else if ($(this).index() == 3) {
			var html ='<ul class="smallSlider clearf slider2">'+ $("#picture_4").html()+'</ul>'
    		$(this).parents(".w_head_main").next().find(".l-course-box").append(html);
    	}
    	var sliderSm = $(".slider2").bxSlider({
          slideWidth: 260,
          auto: true,
          infiniteLoop: true,
          autoControls: true,
          pager: false,
          moveSlides: 1,
          minSlides: 4,
          maxSlides: 4,
          pause: 3000,
          slideMargin: 20
        });
       setTimeout(function () {
        if (sliderSm.getSlideCount() <= 4) {
          sliderSm.destroySlider();
        }
      }, 10);
  })
});

