        <script>
            $("#search").keypress(function (e) {
                if (e.which == 13) {
                    toSearch();
                }
            });

            function toSearch() {
                var words = $("#search").val();
                window.open("https://cn.bing.com/search?q=" + words + "+site%3Agradschool.ustc.edu.cn",
                    "_self");
            }
        </script>
