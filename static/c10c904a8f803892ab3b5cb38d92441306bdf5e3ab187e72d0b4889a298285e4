        <script>
            $('#search').on('focus', function () {
                $('#placeholder').hide();
            }).on('blur', function () {
                if ($(this).val() === '') {
                    $('#placeholder').show();
                }
            });
            $('#placeholder').click(function () {
                console.log(66);
            })
        </script>
