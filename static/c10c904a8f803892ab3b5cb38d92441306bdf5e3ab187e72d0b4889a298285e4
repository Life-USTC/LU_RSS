    <script>
        var ctx = "";

        function clickHref(target, linkType, link, source, id) {
            console.log(target);
            console.log(linkType);
            console.log(link);
            console.log(source);
            console.log(id);
            //文章
            if (linkType == '1') {
                window.open(ctx + "/article/" + id, target);
            }
            //文件
            if (linkType == '2') {
                window.open(source);
            }
            //外链
            if (linkType == '3') {
                window.open(link, target);
            }
        }
    </script>
